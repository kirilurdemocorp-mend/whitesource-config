      },
      {
        "matchDatasources": [
          "go"
        ],
        "matchUpdateTypes": [
          "pinDigest"
        ],
        "enabled": false
      }
    ],
    "minimumReleaseAge": "3 days",
    "prConcurrentLimit": 3,
    "separateMinorPatch": true,
    "separateMultipleMajor": true,
    "kubernetes": {
      "fileMatch": [
        "^\\.vonage/deploy/.*application\\.yaml$"
      ]
    },
