    "extends": [
        ":enableRenovate",
        ":rebaseStalePrs",
        ":semanticCommits",
        "config:recommended",
        "docker:enableMajor",
        "mergeConfidence:all-badges",
        "group:monorepos",
        "group:recommended",
        "replacements:all",
        "workarounds:all"
    ],
    "timezone": "UTC",
